{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "type": "firefox",
            "request": "launch",
            "name": "Firefox",
            "url": "https://localhost:3000",
            "webRoot": "${workspaceFolder}/web"
        }
    ]
}